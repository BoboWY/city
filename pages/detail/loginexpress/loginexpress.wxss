.header_title{
    width: 190rpx;
    height: 47rpx;
    margin: 80rpx auto;
}
.item_title{
    width: 100%;
    height: 100%;
}
/* 登录信息 */
.user_login{
  width: 95%;
  margin: 0 auto;
}
.item_login{
  height: 88rpx;
  line-height: 88rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #E5E5E5;
}
.item_login_icon,.item_icon{
  width: 54rpx;
  height: 54rpx;
  vertical-align: middle;
}
.item_icon{
  position: absolute;
  right: 30rpx;
}
/* 登录按钮 */
.login_btn{
  width: 95%;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  border-radius: 6rpx;
  margin: 80rpx auto;
  font-size: 28rpx;
  color: #fff;
  background-color: #FFA319;
}